{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "rewrites":[
    {
      "source": "/custom-path",
      "destination":"/index.js"
    }
  ],
  "redirects":[
    {
      "source": "/old-path",
      "destination":"/new-path",
      "permanent":true
    }
  ],
  
}
